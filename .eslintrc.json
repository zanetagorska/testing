{
  "extends": ["airbnb","prettier", "preetier/react"],
  "env": {
    "browser": true,
    "commonjs": true,
    "node": true,
    "es6": true
  },
  "parserOptions": {
    "ecmaVersion": 2018
  },
  "rules": {
    "no-console": "off",
    "strict": ["error", "global"],
    "curly": "warn"
  },
  "prettier/prettier": [
    "error",
    {
      "trailingComa": "es5",
      "singleQuote": true,
      "printWidth": 120
    }
  ],
  "plugins": ["prettier"]
}
